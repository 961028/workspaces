{
    "manifest_version": 2,
    "name": "Workspace Manager",
    "version": "1.0",
    "description": "Save and restore sets of tabs as workspaces.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "Workspace Manager",
        "default_popup": "popup/popup.html"
    }
}