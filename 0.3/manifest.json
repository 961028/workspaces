{
  "manifest_version": 2,
  "name": "Workspace Manager",
  "version": "1.0",
  "description": "Save and restore sets of tabs as workspaces.",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "utils.js",
      "handlers.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Workspace Manager",
    "default_popup": "popup/popup.html"
  }
}